{
    "name": "alexjoffroy/laravel-route-localization",
    "description": "A Laravel package to handle localization with ease",
    "keywords": [
        "alexjoffroy",
        "laravel-route-localization",
        "laravel",
        "route",
        "localization",
        "lang",
        "php"
    ],
    "homepage": "https://github.com/alexjoffroy/laravel-route-localization",
    "license": "MIT",
    "authors": [
        {
            "name": "Alex Joffroy",
            "email": "hello@alexjoffroy.me",
            "homepage": "https://alexjoffroy.me",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1"
    },
    "require-dev": {
        "orchestra/testbench": "~3.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "AlexJoffroy\\RouteLocalization\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AlexJoffroy\\RouteLocalization\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "AlexJoffroy\\RouteLocalization\\Providers\\RouteLocalizationServiceProvider"
            ],
            "aliases": {
                "RouteLocalization": "AlexJoffroy\\RouteLocalization\\Facades\\Localization"
            }
        }
    }
}
